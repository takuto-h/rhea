def zero = ^(f, g):
  f()
end

def succ = ^(n):
  ^(f, g):
    g(n)
  end
end

def one = succ(zero)
def two = succ(one)
def three = succ(two)

def add = ^(m, n):
  m^():
    n
  succ^(k):
    succ(add(k, n))
  end
end

